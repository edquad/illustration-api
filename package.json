{
  "name": "vite-react-spa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest --watch --coverage",
    "api:dev": "cross-env NODE_ENV=dev tsx src/api/app.ts",
    "api:uat": "cross-env NODE_ENV=uat tsx src/api/app.ts",
    "api:prod": "cross-env NODE_ENV=prod tsx src/api/app.ts",
    "api:start": "tsx src/api/app.ts",
    "api:build": "tsc --project tsconfig.api.json",
    "start": "concurrently \"npm run dev\" \"npm run api:dev\""
  },
  "dependencies": {
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "snowflake-sdk": "^2.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.18.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.1.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  },
  "author": "",
  "license": "ISC"
}
